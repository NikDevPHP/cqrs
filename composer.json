{
    "name": "carnage/cqrs",
    "description": "Cqrs/Es module for zf2",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "zendframework/zend-log": "^2.5 || ^3.0",
        "zendframework/zend-mvc": "^2.5 || ^3.0",
        "zendframework/zend-servicemanager": "^2.5 || ^3.0",
        "ircmaxell/random-lib": "^1.0.0",
        "doctrine/dbal": "^2.5.1",
